<table class="table table-bordered">
    {{#each this}}
    <tr>
        <td>
            <input {{#if exclude}}disabled{{/if}} class="relationpopup-radio" type="radio" name="elementId" value="{{id}}" data-txt="{{__text__}}"></input>
        </td>
        {{#each this}}
        <td>
            {{this}}
        </td>
        {{/each}}
    </tr>
    {{/each}}
</table>
